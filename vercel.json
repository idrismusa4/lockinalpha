{
  "version": 2,
  "buildCommand": "npm run vercel:build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "api/generateVideo/*.js": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1",
    "FFMPEG_WASM_MODE": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
} 